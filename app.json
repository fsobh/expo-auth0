{
  "expo": {
    "name": "masareef",
    "slug": "masareef",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/Masareef-logo-blue.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.masareef.io"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.masareef.io"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "react-native-auth0",
        {
          "domain": "dev-t5okv6tec0yw3l52.us.auth0.com"
        }
      ],
      [
        "expo-dev-launcher",
        {
          "launchMode": "most-recent"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2d5acb2d-8f4e-4947-b993-a0a6b946ce40"
      }
    }
  }
}
